<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
   
    <dict>
        <!-- Contact Permission -->
        <key>NSContactsUsageDescription</key>
        <string>Your app requires access to your contacts to provide relevant functionality.</string>
        
        <!-- Location Permissions -->
        <key>NSLocationWhenInUseUsageDescription</key>
        <string>Your app requires access to your location while in use to provide relevant features.</string>
        <key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
        <string>Your app requires access to your location to provide continuous updates and personalized content.</string>
        <key>NSLocationAlwaysUsageDescription</key>
        <string>Your app requires access to your location even when it's in the background to provide enhanced features.</string>
        
        <!-- Camera Permission -->
        <key>NSCameraUsageDescription</key>
        <string>Your app requires access to the camera to capture photos and videos.</string>
        
        <!-- Photo Library Permission -->
        <key>NSPhotoLibraryUsageDescription</key>
        <string>Your app requires access to the photo library to select and share photos.</string>
        
        <key>UIApplicationSceneManifest</key>
        <dict>
            <key>UIApplicationSupportsMultipleScenes</key>
            <false/>
            <key>UISceneConfigurations</key>
            <dict>
                <key>UIWindowSceneSessionRoleApplication</key>
                <array>
                    <dict>
                        <key>UISceneConfigurationName</key>
                        <string>Default Configuration</string>
                        <key>UISceneDelegateClassName</key>
                        <string>$(PRODUCT_MODULE_NAME).SceneDelegate</string>
                        <key>UISceneStoryboardFile</key>
                        <string>Main</string>
                    </dict>
                </array>
            </dict>
        </dict>
    </dict>
    
</plist>
